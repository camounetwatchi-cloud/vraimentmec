# .ebextensions/01_packages.config
option_settings:
  # Configuration du chemin WSGI pour le serveur d'application (Gunicorn)
  # NOTE: Utilisez 2 espaces pour l'indentation ici et dans les lignes suivantes
  aws:elasticbeanstalk:container:python:
    WSGIPath: 'application:application'
    
# Installation des paquets système nécessaires à la compilation
packages:
  yum:
    # Les paquets Yum doivent être déclarés en tant qu'éléments dans une liste
    gcc-c++: []
    libevent-devel: []
    python3-devel: []
