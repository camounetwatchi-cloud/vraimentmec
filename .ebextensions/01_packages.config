# .ebextensions/01_packages.config
option_settings:
  aws:elasticbeanstalk:environment:
    # Définit une variable d'environnement pour Gunicorn si vous l'utilisez
    WSGIPath: 'application:application' # MODIFIEZ CECI SI VOTRE FICHIER PRINCIPAL N'EST PAS 'application.py'

packages:
  yum:
    gcc-c++: []
    libevent-devel: []
    python3-devel: []
    # Ajoutez d'autres dépendances de compilation si nécessaire, par exemple :
    # libpq-devel: [] # Pour les drivers PostgreSQL
